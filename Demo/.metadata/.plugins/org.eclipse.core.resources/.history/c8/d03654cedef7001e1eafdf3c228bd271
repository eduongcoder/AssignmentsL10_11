package entity;

import java.sql.Date;

public class user {
    private short id;
    private String username;
    private String email;
    private String password;
    private String firstName;
    private String lastName;
    private char gender; 
    private java.sql.Date dateOfBirth;
    private String address;
    private String phone;
    private String role;
    private byte status;
	public short getId() {
		return id;
	}
	public String getUsername() {
		return username;
	}
	public String getEmail() {
		return email;
	}
	public String getPassword() {
		return password;
	}
	public String getFirstName() {
		return firstName;
	}
	public String getLastName() {
		return lastName;
	}
	public char getGender() {
		return gender;
	}
	public java.sql.Date getDateOfBirth() {
		return dateOfBirth;
	}
	public String getAddress() {
		return address;
	}
	public String getPhone() {
		return phone;
	}
	public String getRole() {
		return role;
	}
	public byte getStatus() {
		return status;
	}
	public user(short id, String username, String email, String password, String firstName, String lastName,
			char gender, Date dateOfBirth, String address, String phone, String role, byte status) {
		super();
		this.id = id;
		this.username = username;
		this.email = email;
		this.password = password;
		this.firstName = firstName;
		this.lastName = lastName;
		this.gender = gender;
		this.dateOfBirth = dateOfBirth;
		this.address = address;
		this.phone = phone;
		this.role = role;
		this.status = status;
	}
	@Override
	public String toString() {
		return "user [id=" + id + ", username=" + username + ", email=" + email + ", password=" + password
				+ ", firstName=" + firstName + ", lastName=" + lastName + ", gender=" + gender + ", dateOfBirth="
				+ dateOfBirth + ", address=" + address + ", phone=" + phone + ", role=" + role + ", status=" + status
				+ "]";
	}
    
    
}
